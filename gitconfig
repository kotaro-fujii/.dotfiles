[include]
    path = ~/.profile.gitconfig

[init]
    defaultBranch = main
[alias]
    pl = pull
    ps = push
    st = status
    cm = commit
    ck = checkout
    sw = switch
    df = diff
    ca = commit -a
    ad = add
    ada = add .
    br = branch
    ls = ls-files
[color]
    ui = auto
[core]
    editor = vim
